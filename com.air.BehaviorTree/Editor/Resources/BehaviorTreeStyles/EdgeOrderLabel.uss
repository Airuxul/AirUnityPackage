/* Execution order label on edges with [OrderByPosition] output */
.bt-edge-order-label {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 18px;
    height: 18px;
    margin-left: -9px;
    margin-top: -9px;
    -unity-text-align: middle-center;
    font-size: 11px;
    background-color: rgba(51, 51, 51, 0.9);
    border-radius: 9px;
    color: white;
    display: none;
}

.bt-edge-order-label.visible {
    display: flex;
}
